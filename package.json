{
  "name": "gosh-retro-launcher",
  "version": "1.1.1",
  "description": "A Windows 3.1 Program Manager clone built with Tauri and Svelte",
  "type": "module",
  "author": "Goshitsarch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/goshitsarch-eng/Gosh-Retro-Launcher"
  },
  "scripts": {
    "dev": "tauri dev",
    "dev:frontend": "vite",
    "build": "tauri build",
    "build:frontend": "vite build",
    "build:win": "tauri build --target x86_64-pc-windows-msvc",
    "build:mac": "tauri build --target universal-apple-darwin",
    "build:mac-arm": "tauri build --target aarch64-apple-darwin",
    "build:mac-intel": "tauri build --target x86_64-apple-darwin",
    "build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2",
    "@tauri-apps/plugin-fs": "^2",
    "@tauri-apps/plugin-global-shortcut": "^2",
    "@tauri-apps/plugin-process": "^2",
    "@tauri-apps/plugin-shell": "^2",
    "@tauri-apps/plugin-store": "^2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6",
    "@tauri-apps/cli": "^2",
    "@types/node": "^22",
    "@types/uuid": "^9.0.7",
    "svelte": "^5",
    "svelte-check": "^4",
    "typescript": "^5.7",
    "vite": "^7"
  }
}
